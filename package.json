{
	"name": "testman",
	"version": "0.0.11",
	"license": "MIT",
	"description": "Expiremental testing helper.",
	"main": "index.js",
	"author": "Lauri Rooden <lauri@rooden.ee>",
	"repository": {
		"type": "git",
		"url": "git://github.com/lauriro/testman.git"
	},
	"bugs": {
		"url": "https://github.com/lauriro/testman/issues"
	},
	"scripts": {
		"travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
		"test": "node tests/run.js"
	},
	"keywords": [
		"test",
		"tap"
	],
	"testling": {
		"files": "tests/run.js",
		"browsers": {
			"ie": [ 6, 7, 8, 9, 10 ],
			"ff": [ 3, 3.6, 4, 18, 19, "nightly" ],
			"chrome": [ 4, 10, 20, 24, 25, "canary" ],
			"safari": [ 5.1, 6 ],
			"opera": [ 10, 11, "11.6", 12, "next" ],
			"android-browser": [ 4.2 ],
			"iphone": [ 6 ],
			"ipad": [ 6 ]
		}
	},
	"readmeFilename": "README.md"
}
