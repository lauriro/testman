{
  "name": "testman",
  "version": "0.1.8",
  "stability": 2,
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "Expiremental testing helper.",
  "keywords": [
    "test",
    "tap"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "node node_modules/buildman/index.js --all",
    "travis-test": "istanbul cover ./tests/run.js && (coveralls < coverage/lcov.info || exit 0)",
    "test": "node tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lauriro/testman.git"
  },
  "bugs": {
    "url": "https://github.com/lauriro/testman/issues"
  },
  "devDependencies": {
    "buildman": "*",
    "browser-upgrade-lite": "*"
  },
  "buildman": {
    "min.js": "index.js"
  },
  "testling": {
    "files": "tests/run.js",
    "browsers": [
      "ie/6..10",
      "ff/3",
      "ff/4",
      "ff/5",
      "ff/20",
      "ff/25",
      "chrome/4",
      "chrome/10",
      "chrome/20",
      "chrome/30",
      "chrome/31",
      "opera/10",
      "opera/11",
      "opera/11.6",
      "opera/12",
      "opera/16",
      "safari/4..6",
      "android-browser/4.2",
      "iphone/6",
      "ipad/6"
    ]
  }
}
